{
    "name": "mvdnbrk/gtin",
    "description": "GTIN / EAN / UPC Validation for Laravel",
    "keywords": [
        "laravel",
        "validation",
        "validator",
        "ean",
        "upc",
        "gtin",
        "gs1"
    ],
    "homepage": "https://github.com/mvdnbrk/gtin",
    "license": "MIT",
    "authors": [
        {
            "name": "Mark van den Broek",
            "email": "mvdnbrk@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "5.5.* || 5.6.* || 5.7.* || 5.8.* || ^6.0 || ^7.0",
        "illuminate/validation": "5.5.* || 5.6.* || 5.7.* || 5.8.* || ^6.0 || ^7.0"
    },
    "require-dev": {
        "doctrine/dbal": "^2.9",
        "orchestra/testbench": "^3.5 || ^3.6 || ^3.7 || ^3.8 || ^4.0 || ^5.0",
        "phpunit/phpunit": "^6.0 || ^7.0 || ^8.0 || ^9.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mvdnbrk\\Gtin\\GtinServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Mvdnbrk\\Gtin\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Mvdnbrk\\Gtin\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
